{
  "name": "harpnative",
  "version": "0.2.0",
  "description": "A React Native implementation of the HarpGuru concept; a UI to aid the understanding of harmonica music theory",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test": "jest",
    "lint": "eslint --ext .ts,.tsx .",
    "ci": "yarn lint && yarn tsc",
    "prettier": "prettier --write .",
    "build-android-aad": "expo build:android -t app-bundle --clear-credentials",
    "build-android-apk": "expo build:android -t apk --clear-credentials"
  },
  "dependencies": {
    "expo": "^38.0.0",
    "expo-screen-orientation": "~1.1.1",
    "harpguru": "^0.1.0",
    "harpstrata": "^6.3.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "6.5.1",
    "eslint-plugin-import": "^2.21.2",
    "prettier": "^2.0.5",
    "typescript": "~3.9.5"
  },
  "private": true
}
