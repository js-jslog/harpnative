{
  "name": "harpguru-expo-boilerplate",
  "version": "0.1.0",
  "description": "An expo based boilerplate to run the harpguru-core app",
  "main": "__generated__/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "eslint --ext .ts,.tsx .",
    "ci": "yarn lint && yarn tsc",
    "prettier": "prettier --write .",
    "build-android-aad": "expo build:android -t app-bundle --clear-credentials",
    "build-android-apk": "expo build:android -t apk --clear-credentials",
    "postinstall": "expo-yarn-workspaces postinstall"
  },
  "dependencies": {
    "expo": "^38.0.0",
    "expo-screen-orientation": "~1.1.1",
    "harpguru-core": "0.2.2",
    "react": "16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@types/react": "~16.9.41",
    "@types/react-native": "~0.62.13",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "babel-preset-expo": "^8.2.3",
    "eslint": "6.5.1",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-native": "^3.8.1",
    "expo-yarn-workspaces": "^1.2.1",
    "prettier": "^2.0.5",
    "typescript": "~3.9.5"
  },
  "private": true
}
